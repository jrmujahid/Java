/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	Scanner jamil=new Scanner(System.in);
	int X;
	X=jamil.nextInt();

	float amount;
	amount=jamil.nextFloat();

	if(amount>=(X*5))
	{
	    float withdraw=amount-X;
	    withdraw -=0.50;
	    System.out.println("\n\n"+withdraw);
	    
	}
	else if(X>amount){
	
	    System.out.println("\n\n"+amount);
	}
	
	}
}
