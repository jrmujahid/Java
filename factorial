
import java.util.*;
public class Main
{
  public static void main (String[]args)
  {

    Scanner jamil = new Scanner (System.in);
    int b=jamil.nextInt();

 
 for (int i=0;i<=b;i++ ){
    int scanner=jamil.nextInt();
      int sum=1;
    while(scanner!=0){
 
    sum=scanner*sum;
     scanner--;
    }
    System.out.println(sum);
 } 

  }
}
